    "chai": "^1.10.0",
    "chai-as-promised": "^4.1.1",
    "cpy": "^3.4.0",
    "eslint": "^0.24.1",
    "imports-loader": "^0.6.3",
    "karma": "~0.12.23",
    "karma-mocha": "~0.1.9",
    "karma-mocha-reporter": "^0.3.1",
    "karma-phantomjs-launcher": "~0.1.4",
    "karma-sourcemap-loader": "^0.3.2",
    "karma-webpack": "~1.2.2",
    "mocha": "~1.21.4",
    "phantomjs": "^1.9.16",
    "react": ">=0.11.0 || 0.14.0-beta3",
    "rimraf": "^2.2.8",
    "sinon-chai": "^2.6.0",
    "style-loader": "^0.8.3",
    "webpack": "^1.4.15",
    "webpack-dev-server": "^1.7.0"
